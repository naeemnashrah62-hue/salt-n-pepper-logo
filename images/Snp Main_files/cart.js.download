function checkOrderLimit(productJson, clickedElement) {

  // Get the order limit for the product from the productJson
  let orderLimit = productJson.order_limit;

  // If order limit is not present, try to retrieve and decrypt the product detail from the UI
  if (!orderLimit) {
    // Try _product_main first (used in customCards.phtml)
    let productDetail = $("#" + productJson.menu_item_id + "_product_main").val();

    // If not found, try _product (used in productCard.phtml, featuredProducts.phtml)
    if (!productDetail) {
      productDetail = $("#" + productJson.menu_item_id + "_product").val();
    }

    // If still not found, log warning and allow order (no limit)
    if (!productDetail) {
      console.warn('checkOrderLimit: Product detail field not found for menu_item_id:', productJson.menu_item_id);
      return true;
    }

    // Decrypt the product detail
    const decryptedProduct = decryptJson(productDetail);

    // If decryption failed, allow order (no limit)
    if (!decryptedProduct) {
      console.warn('checkOrderLimit: Failed to decrypt product detail for menu_item_id:', productJson.menu_item_id);
      return true;
    }

    productJson = decryptedProduct;
    orderLimit = productJson.order_limit;
  }

  // Log the order limit for debugging
  console.log('Order limit:', orderLimit);

  // If the order limit is 0 or less, allow the order
  if (orderLimit <= 0) {
    return true;
  }

  // Retrieve the current cart data from localStorage, or set it as an empty object if not found
  let cartData = JSON.parse(localStorage.getItem("cart")) || { items: [] };

  // Get the current selected quantity near the clicked button (e.g., for an increment/decrement action)
  let currentSelectedQty = parseFloat($(clickedElement).siblings(".total_qty").text());

  // If the clicked element is an increment button, override the selected quantity
  if ($(clickedElement).hasClass("increment_btn") ) {
    currentSelectedQty = parseFloat($(".detail-page-qty").text());
  }

  // Find the item in the cart by matching menu_item_id
  let itemInCart = cartData.items.find(item => item.menu_item_id == productJson.menu_item_id);

  // If the item exists in the cart, use its quantity (dqty), else default to 0
  let cartQty = itemInCart ? parseFloat(itemInCart.dqty) || 0 : 0;

  // Calculate the total quantity by adding the current selected quantity to the quantity in the cart

  if (isNaN(currentSelectedQty)) {
      currentSelectedQty = 0;
  }

  let totalQty = cartQty + currentSelectedQty;

  // If the clicked element is an "addition" button, set total quantity to only the cart quantity (don't include the selected qty)
  if ($(clickedElement).hasClass("addition")) {
    totalQty = cartQty;
  }

  // Check if the total quantity exceeds the order limit
  if (totalQty >= orderLimit) {
    // If the limit is reached, show a warning and prevent the order
    showStatus("Order limit reached");
    return false;
  }

  // If the order limit is not reached, allow the order
  return true;
}


$(document).on("click", ".wishlist-icon", function(){
  let itemId = $(this).data("item-id");
  let $btn = $(this);

  // Check if item is already in wishlist (red color means it's in wishlist)
  let isInWishlist = $btn.css("color") === "rgb(255, 0, 0)" || $btn.css("color") === "red";

  // Check if we're on the wishlist page
  let isWishlistPage = window.location.pathname.includes('/wishlist');

  $btn.html("<div style='width: 16px !important; height: 16px !important;' class='spinner-border loader_popup' role='status'> <span class='sr-only'>Loading...</span></div>");

  if (isInWishlist) {
    // Remove from wishlist
    fetch(baseURL+"/api/removeWishlist", {
      method: "POST",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({ item_id: itemId }),
    })
      .then((response) => response.json())
      .then((data) => {
        console.log(data);

        if (data.success) {
          // Remove from localStorage
          let wishlist = JSON.parse(localStorage.getItem("wishlist")) || [];
          wishlist = wishlist.filter(item => item.item_id != itemId);
          localStorage.setItem("wishlist", JSON.stringify(wishlist));

          sendUiresponse("Item removed from wishlist");

          // If on wishlist page, remove the product card from DOM
          if (isWishlistPage) {
            // Find and remove the product card
            $btn.closest('.single_product_theme').fadeOut(300, function() {
              $(this).remove();

              // Check if there are any items left in the wishlist container
              if ($('#wishlist-items-container .single_product_theme').length === 0) {
                // Show empty wishlist message
                $('#wishlist-items-container').html('<div class="col-12 text-center"><p class="mb-3">Your wishlist is empty</p><a href="/" class="btn btn-dark">Continue Shopping</a></div>');
              }
            });
          } else {
            // Update UI - reset to original color
            $btn.html("<i class='fas fa-heart'></i>");
            $btn.css("color", "");
          }
        }
      })
      .catch((error) => {
        console.error("Error:", error);
        $btn.html("<i class='fas fa-heart'></i>");
      });
  } else {
    // Add to wishlist
    fetch(baseURL+"/api/addWishlist", {
      method: "POST",
      headers: {
        "Content-Type": "application/json",
      },
      body: JSON.stringify({ item_id: itemId }),
    })
      .then((response) => response.json())
      .then((data) => {
        console.log(data);

        // Assuming 'data' is your API response
        if (data.success && data.data && data.data.result) {
          const newItem = data.data.result;

          // Get existing wishlist from localStorage
          let wishlist = JSON.parse(localStorage.getItem("wishlist")) || [];

          // Ensure it's an array
          if (!Array.isArray(wishlist)) {
            wishlist = [wishlist];
          }

          // Append new item (and prevent duplicates if needed)
          const exists = wishlist.some(item => item.item_id === newItem.item_id);
          if (!exists) {
            wishlist.push(newItem);
          }

          // Save updated wishlist
          localStorage.setItem("wishlist", JSON.stringify(wishlist));

          // Update UI - set to red
          $btn.html("<i class='fas fa-heart'></i>");
          $btn.css("color", "red");
          sendUiresponse("Item added to wishlist");
        }
      })
      .catch((error) => {
        console.error("Error:", error);
        $btn.html("<i class='fas fa-heart'></i>");
      });
  }
})

function sendUiresponse(data) {
    showStatus(data);
}