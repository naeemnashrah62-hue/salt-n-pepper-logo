    let productDomedeatil = $(".product_detail").val(); 
    try {
        productJson = decryptJson(productDomedeatil);

      } catch (e) {
        console.error("Failed to parse product detail JSON:", e);
      }
  }
}

function decryptJson(encodedData) {
  try {
    // Check if encodedData is valid
    if (!encodedData || encodedData === '' || encodedData === 'undefined') {
      console.warn('decryptJson: Invalid or empty encoded data');
      return null;
    }

    const decodedData = atob(encodedData);
    return JSON.parse(decodedData);
  } catch (error) {
    console.error('decryptJson: Failed to decode data', error);
    return null;
  }
}

function sidebarDecrypt(encodedData) {
  try {
    // Check if encodedData is valid
    if (!encodedData || encodedData === '' || encodedData === 'undefined') {
      console.warn('sidebarDecrypt: Invalid or empty encoded data');
      return null;
    }

    // Decode: Base64(URL-encoded JSON) -> URL-encoded JSON -> JSON
    const decodedData = decodeURIComponent(atob(encodedData));
    return JSON.parse(decodedData);
  } catch (error) {
    console.error('sidebarDecrypt: Failed to decode data', error);
    console.error('sidebarDecrypt: Encoded data preview:', encodedData.substring(0, 50) + '...');
    return null;
  }
}

