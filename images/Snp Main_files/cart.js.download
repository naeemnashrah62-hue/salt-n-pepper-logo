// maintain state
function state_maintain() {
  try {
    // Handle cart data
    let cartData = JSON.parse(localStorage.getItem("cart")) || [];

    if (cartData && cartData.items && Array.isArray(cartData.items)) {
      cartData.items.forEach(element => {
        try {
          let id = element.menu_item_id;
          let qty = element.dqty;
          let optionSet = JSON.parse(element.option_set || '{}');
          let hasOptions = Object.keys(optionSet).length > 0;
          let cartBtn = $(".cart_button_" + id);

          if (!hasOptions) {
            cartBtn.siblings(".total_qty_product").html(qty).removeClass("hide");
            cartBtn.siblings(".total_qty_product")
              .siblings(".reduce_qty, .add_to_cart")
              .addClass("qty_increased");
            cartBtn.siblings(".total_qty_product")
              .siblings(".reduce_qty")
              .removeClass("hide");

            // Handle button text for items with custom labels
            let increaseBtn = $(".cart_button_" + id);
            if (increaseBtn.hasClass("check-lable-applied-or-not")) {
              increaseBtn.html("<i class='fas fa-plus'></i>");
            }
          }
        } catch (err) {
          console.error("Error processing cart item:", err);
        }
      });
    }

    // Handle wishlist data
    try {
      let wishlistData = JSON.parse(localStorage.getItem("wishlist")) || [];
      console.log( "After calling", wishlistData);

      wishlistData.forEach(element => {
        try {
          let id = element.item_id;
          let wishlistBtn = $(".wishlist-data-" + id);
          wishlistBtn.html("<i class='fas fa-heart'></i>");
          wishlistBtn.css("color", "red");
        } catch (err) {
          console.error("Error processing wishlist item:", err);
        }
      });
    } catch (err) {
      console.error("Error reading wishlist data:", err);
    }
  } catch (err) {
    console.error("Error in state_maintain():", err);
  }
}


